<?xml version="1.0"?>
<!-- This configuration file supports some kind of inheritment.
  You can overwrite, add things or purge
  To purge,only write your element in the purge node like the others and read the code to see what you can do
-->
<root>
  <template>
    <versions>
      <!-- z3c.form -->
      <version p='plone.z3cform'           v='0.7.3'   name="various pinning for z3 packages" />
      <version p="zope.schema"             v="3.7.1"   name="various pinning for z3 packages"/>
    </versions>
    <checkedversions>
      <version p='archetypes.schemaextender' v='2.1'   options='with_ploneproduct_ploneappblob,with_ploneproduct_schemaextender,with_ploneproduct_cynin' />
    </checkedversions>
  </template>
  <purge>
    <versions>
      <version p='plone.z3cform'           v='0.7.1'   name="various pinning for z3 packages" />
      <version p="zope.schema"             v="3.6.1"   name="various pinning for z3 packages"/> 
    </versions>
    <checkedversions>
      <version p='archetypes.schemaextender' options='with_ploneproduct_ploneappblob,with_ploneproduct_schemaextender,with_ploneproduct_cynin' />
    </checkedversions>
  </purge>
</root>
